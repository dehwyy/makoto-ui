{
	"name": "makoto-ui-vue",
	"description": "UI library for http://github.com/dehwyy/Makoto",
	"homepage": "https://github.com/dehwyy/makoto-ui",
	"author": "dehwyy <https://github.com/dehwyy>",
	"version": "0.0.7",
	"license": "MIT",
	"exports": {
		".": {
			"types": "./src/vue/index.d.ts",
			"import": "./src/vue/index.js"
		},
		"./css": {
			"import": "./src/vue/style.css"
		}
	},
	"scripts": {
		"minify": "minify-all-js ./src/vue"
	},
	"main": "src/vue/index.js",
	"files": [
		"src/vue/*.js",
		"src/vue/*.d.ts",
		"src/vue/style.css"
	],
	"types": "src/vue/index.d.ts",
	"type": "module",
	"module": "src/vue/index.js"
}
