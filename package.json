{
	"name": "makoto-ui",
	"description": "UI library for http://github.com/dehwyy/Makoto",
	"homepage": "https://github.com/dehwyy/makoto-ui",
	"author": "dehwyy <https://github.com/dehwyy>",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"package": "svelte-kit sync && svelte-package -o package/src && publint",
		"prepub": "npm run package",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"exports": {
		".": {
			"types": "./package/src/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^7.4.5",
		"@storybook/addon-interactions": "^7.4.5",
		"@storybook/addon-links": "^7.4.5",
		"@storybook/blocks": "^7.4.5",
		"@storybook/svelte": "^7.4.5",
		"@storybook/sveltekit": "^7.4.5",
		"@storybook/testing-library": "^0.2.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.0",
		"css-minimizer-webpack-plugin": "^5.0.1",
		"mini-css-extract-plugin": "^2.7.6",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.9",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.4.5",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-loader": "^3.1.9",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.34.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.4.6"
	}
}
